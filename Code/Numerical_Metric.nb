Cell[TextData[StyleBox["Write a function that imputs a set of values G_j1j2 \
on the grid of a source and an operator on the grid M_i1j1i2j2 and converts \
these to vectors indicexed by a superindex A and B, solves the equations \
M_ABv_B=G_A a and then converts v_A to a set of values of the grid. CHECK \
this",
 FontSize->16]], "Text",
 CellChangeTimes->{{3.729530950948936*^9, 3.72953112031382*^9}, {
  3.73016355332412*^9, 3.730163560256852*^9}, {3.7319649276583567`*^9, 
  3.731964929634737*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"solver", "[", 
   RowBox[{"Mgrid_", ",", "Ggrid_"}], "]"}], ":=", 
  RowBox[{"Block", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"N1", ",", "N2", ",", "Gsup", ",", "Msup", ",", "vsup"}], "}"}], 
    ",", "\[IndentingNewLine]", 
    RowBox[{"(*", 
     RowBox[{
      RowBox[{"read", " ", "off", " ", "N1", " ", "and", " ", "N2"}], ",", 
      " ", 
      RowBox[{
      "taking", " ", "into", " ", "account", " ", "there", " ", "are", " ", 
       "N1"}], ",", " ", 
      RowBox[{
       RowBox[{"and", " ", "N2"}], "+", 
       RowBox[{
       "1", " ", "grid", " ", "points", " ", "in", " ", "each", " ", 
        "direction"}]}]}], "*)"}], "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"N1", "=", 
      RowBox[{
       RowBox[{
        RowBox[{"Dimensions", "[", "Ggrid", "]"}], "[", 
        RowBox[{"[", "1", "]"}], "]"}], "-", "1"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"N2", "=", 
      RowBox[{
       RowBox[{
        RowBox[{"Dimensions", "[", "Ggrid", "]"}], "[", 
        RowBox[{"[", "2", "]"}], "]"}], "-", "1"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{"Create", " ", "super", " ", "vectors"}], ",", " ", 
       RowBox[{
       "taking", " ", "into", " ", "account", " ", "that", " ", "the", " ", 
        "indices", " ", "which", " ", "label", " ", "the", " ", "matrix", " ",
         "are", " ", "one", " ", "larger", " ", "than", " ", "the", " ", 
        "indices", " ", "which", " ", "label", " ", "the", " ", "list", " ", 
        "for", " ", "the", " ", "angular", " ", "direction"}]}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"Gsup", "=", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"Ggrid", "[", 
         RowBox[{"[", 
          RowBox[{
           RowBox[{
            RowBox[{
             RowBox[{"n1sub", "[", "N1", "]"}], "[", "A", "]"}], "+", "1"}], 
           ",", 
           RowBox[{
            RowBox[{
             RowBox[{"n2sub", "[", "N1", "]"}], "[", "A", "]"}], "+", "1"}]}],
           "]"}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"A", ",", "0", ",", 
          RowBox[{
           RowBox[{
            RowBox[{"(", 
             RowBox[{"N1", "+", "1"}], ")"}], 
            RowBox[{"(", 
             RowBox[{"N2", "+", "1"}], ")"}]}], "-", "1"}]}], "}"}]}], 
       "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"Msup", "=", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"Mgrid", "[", 
         RowBox[{"[", 
          RowBox[{
           RowBox[{
            RowBox[{
             RowBox[{"n1sub", "[", "N1", "]"}], "[", "A", "]"}], "+", "1"}], 
           ",", 
           RowBox[{
            RowBox[{
             RowBox[{"n1sub", "[", "N1", "]"}], "[", "B", "]"}], "+", "1"}], 
           ",", 
           RowBox[{
            RowBox[{
             RowBox[{"n2sub", "[", "N1", "]"}], "[", "A", "]"}], "+", "1"}], 
           ",", 
           RowBox[{
            RowBox[{
             RowBox[{"n2sub", "[", "N1", "]"}], "[", "B", "]"}], "+", "1"}]}],
           "]"}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"A", ",", "0", ",", 
          RowBox[{
