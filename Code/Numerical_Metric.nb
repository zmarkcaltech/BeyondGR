Cell[TextData[StyleBox["This operator places the two radial  indices first \
and the two angular indices second which differs from my convention in the \
note where I alternate radial and angular indices, i.e we write L_i1j1i2j2= \
L(C_j1(x_i1)*C_j2(x_i2). \n\nAlso the radial and angular indices are shifted \
up by one since they become list indices. Hence\nthe angular grid points are \
defined by c_j with j running from 0 to N2 in the below matrices we label \
them c_j with j running from 1 to N2+1 while the radial gridpoints without \
the first row are defined by h_j with with j  running from 1 to N1.",
 FontSize->16]], "Text",
 CellChangeTimes->{{3.7301608553155003`*^9, 3.730160899887805*^9}, {
  3.73016126784191*^9, 3.730161331585918*^9}, {3.7303380724980803`*^9, 
  3.730338083215664*^9}, {3.73039845565359*^9, 3.73039845758025*^9}, {
  3.730398495228891*^9, 3.730398770922287*^9}, {3.730399019289147*^9, 
  3.730399056519945*^9}, {3.731948043952113*^9, 3.731948044775218*^9}}],

Cell[TextData[StyleBox["We compute the Lop operater using the coordinates x \
and c, i.e. we just use Chebyshevs in the radial direction",
 FontSize->16]], "Text",
 CellChangeTimes->{{3.731948051286322*^9, 3.7319480794519663`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"LopMat", "[", 
   RowBox[{"N1_", ",", "N2_", ",", "L_"}], "]"}], ":=", 
  RowBox[{
   RowBox[{"LopMat", "[", 
    RowBox[{"N1", ",", "N2", ",", "L"}], "]"}], "=", 
   RowBox[{"Block", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"x", "=", 
        RowBox[{"xgrid", "[", "N1", "]"}]}], ",", 
       RowBox[{"c", "=", 
        RowBox[{"cgrid", "[", "N2", "]"}]}], ",", "A", 
       RowBox[{"(*", 
        RowBox[{"coefficients", " ", "of", " ", 
         RowBox[{
          RowBox[{"d", "^", "2"}], "/", 
          RowBox[{"dx", "^", "2"}]}], " ", "evaluated", " ", "on", " ", "the",
          " ", "grid"}], "*)"}], " ", ",", "B"}], "}"}], 
     RowBox[{"(*", 
      RowBox[{"coefficients", " ", "of", " ", 
       RowBox[{"d", "/", "dx"}], " ", "evaluated", " ", "on", " ", "the", " ",
        "grid"}], "*)"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"A", "=", 
       RowBox[{"DiagonalMatrix", "[", 
        FractionBox[
         RowBox[{
          RowBox[{"(", 
           RowBox[{"1", "+", "x"}], ")"}], 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"L", 
             RowBox[{"(", 
              RowBox[{"1", "+", "x"}], ")"}]}], "+", 
            RowBox[{"2", 
             RowBox[{"(", 
              RowBox[{"1", "-", "x"}], ")"}]}]}], ")"}], 
          RowBox[{
           RowBox[{"(", 
            RowBox[{"1", "-", "x"}], ")"}], "^", "2"}]}], 
         RowBox[{"4", "L"}]], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"B", "=", 
       RowBox[{"DiagonalMatrix", "[", 
        RowBox[{
         FractionBox[
          RowBox[{
           RowBox[{"-", 
            RowBox[{"(", 
             RowBox[{"1", "+", "x"}], ")"}]}], 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"L", 
              RowBox[{"(", 
               RowBox[{"1", "+", "x"}], ")"}]}], "+", 
             RowBox[{"2", 
              RowBox[{"(", 
               RowBox[{"1", "-", "x"}], ")"}]}]}], ")"}], 
           RowBox[{"(", 
            RowBox[{"1", "-", "x"}], ")"}]}], 
          RowBox[{"2", "L"}]], "+", 
         FractionBox[
          RowBox[{"2", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"L", 
              RowBox[{"(", 
               RowBox[{"1", "+", "x"}], ")"}]}], "+", "1", "-", "x"}], ")"}], 
           
           RowBox[{"(", 
            RowBox[{"1", "-", "x"}], ")"}]}], 
          RowBox[{"2", "L"}]]}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"TensorProduct", "[", 
        RowBox[{
